{
  "private": [
    {
      "path": "/echo-param/:param",
      "method": "GET",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/echo-body/:param?",
      "method": "POST",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/echo-headers/:param?",
      "method": "POST",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/echo-query",
      "method": "GET",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/echo-query/:param?",
      "method": "GET",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/test-blob/*",
      "method": "GET",
      "origin": "http://localhost:${originPort}"
    },

    {
      "path": "/test-blob-param/*",
      "method": "GET",
      "origin": "http://localhost:${originPort}"
    },

    {
      "//": "Block on headers",
      "path": "/echo-param-protected/:param",
      "method": "GET",
      "origin": "http://localhost:${originPort}",
      "valid": [
        {
          "header": "accept",
          "values": ["valid.accept.header"]
        }
      ]
    }
  ],
  "public": [
    {
      "path": "/test-blob/*",
      "method": "GET",
      "stream": true
    },
    {
      "path": "/test-blob-param/*",
      "method": "GET",
      "stream": true
    },
    {
      "path": "/client-not-capable",
      "method": "GET",
      "requiredCapabilities": ["no-such-capability"]
    },
    {
      "path": "/*",
      "method": "GET"
    },
    {
      "path": "/*",
      "method": "POST"
    }

  ]
}
